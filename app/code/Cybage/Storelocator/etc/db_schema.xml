<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table comment="cybage_storelocator_dealer Table" engine="innodb" name="cybage_storelocator_dealer" resource="default">
        <column comment="Entity Id" identity="true" name="dealer_id" nullable="false" padding="6" unsigned="false" xsi:type="int"/>
        <constraint referenceId="PRIMARY" xsi:type="primary">
            <column name="dealer_id"/>
        </constraint>
        <column name="group_id" nullable="true" xsi:type="int"/>
        <column name="address" nullable="true" xsi:type="text"/>
        <column name="city_id" nullable="true" xsi:type="int"/>
        <column length="255" name="phone" nullable="true" xsi:type="varchar"/>
        <column name="bajaj_dealerid" nullable="true" xsi:type="int"/>
        <column length="255" name="dealer_name" nullable="true" xsi:type="varchar"/>
        <column length="255" name="area" nullable="true" xsi:type="varchar"/>
        <column length="255" name="pincode" nullable="true" xsi:type="varchar"/>
        <column length="255" name="latitude" nullable="true" xsi:type="varchar"/>
        <column length="255" name="longitude" nullable="true" xsi:type="varchar"/>
        <column name="subcategories" nullable="true" xsi:type="text"/>
        <column name="is_active" nullable="true" xsi:type="boolean"/>
        <column name="lob" nullable="true" xsi:type="text"/>
        <column name="sfdc" nullable="true" xsi:type="boolean" default="0"/>
        <column length="255" name="dealer_logo" nullable="true" xsi:type="varchar"/>
        
    </table>
    <table comment="cybage_storelocator_dealercategory Table" engine="innodb" name="cybage_storelocator_dealercategory" resource="default">
        <column comment="Entity Id" identity="true" name="dealercategory_id" nullable="false" padding="6" unsigned="false" xsi:type="smallint"/>
        <constraint referenceId="PRIMARY" xsi:type="primary">
            <column name="dealercategory_id"/>
        </constraint>
        <column name="dealer_id" nullable="true" xsi:type="int"/>
        <column name="oem_id" nullable="true" xsi:type="int"/>
        <column length="255" name="oem_name" nullable="true" xsi:type="varchar"/>
        <column length="255" name="subcategory_id" nullable="true" xsi:type="varchar"/>
    </table>
    <table comment="cybage_storelocator_dealergroup Table" engine="innodb" name="cybage_storelocator_dealergroup" resource="default">
        <column comment="Entity Id" identity="true" name="dealergroup_id" nullable="false" padding="6" unsigned="false" xsi:type="int"/>
        <constraint referenceId="PRIMARY" xsi:type="primary">
            <column name="dealergroup_id"/>
        </constraint>
        <column name="group_id" nullable="true" xsi:type="int"/>
        <column length="255" name="store_name" nullable="true" xsi:type="varchar"/>
    </table>
    <table comment="cybage_storelocator_city Table" engine="innodb" name="cybage_storelocator_city" resource="default">
        <column comment="Entity Id" identity="true" name="city_id" nullable="false" padding="6" unsigned="false" xsi:type="int"/>
        <constraint referenceId="PRIMARY" xsi:type="primary">
            <column name="city_id"/>
        </constraint>
        <column name="bajaj_city_id" nullable="true" xsi:type="int"/>
        <column length="255" name="city_name" nullable="true" xsi:type="varchar"/>
        <column name="city_latitude" nullable="true" xsi:type="varchar" length="255"/>
        <column name="city_longitude" nullable="true" xsi:type="varchar" length="255"/>
    </table>
    <table comment="cybage_storelocator_offer Table" engine="innodb" name="cybage_storelocator_offer" resource="default">
        <column comment="Entity Id" identity="true" name="offer_id" nullable="false" padding="6" unsigned="false" xsi:type="int"/>
        <constraint referenceId="PRIMARY" xsi:type="primary">
            <column name="offer_id"/>
        </constraint>
        <column name="dealerid" nullable="true" xsi:type="int"/>
        <column name="dealerofferid" nullable="true" xsi:type="int"/>
        <column name="offer_text" nullable="true" xsi:type="text"/>
    </table>
    <table comment="cybage_storelocator_offertype Table" engine="innodb" name="cybage_storelocator_offertype" resource="default">
        <column comment="Entity Id" identity="true" name="offertype_id" nullable="false" padding="6" unsigned="false" xsi:type="int"/>
        <constraint referenceId="PRIMARY" xsi:type="primary">
            <column name="offertype_id"/>
        </constraint>
        <column name="offerid" nullable="true" xsi:type="int"/>
        <column length="255" name="offer_type" nullable="true" xsi:type="varchar"/>
    </table>
</schema>
